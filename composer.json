{
    "name": "netsells/laravel-geoscope",
    "description": "An easy to use trait that can be added to models to allow distance based scopes to be applied",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Hutchison",
            "email": "adam.hutchison@netsells.co.uk"
        }
    ],
    "require": {
        "mockery/mockery": "^1.3"
    },
    "require-dev": {
        "laravel/framework": "^6.6",
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netsells\\GeoScope\\GeoScopeServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Netsells\\GeoScope\\": "src",
            "Netsells\\GeoScope\\Tests\\": "tests"
        }
    }
}
